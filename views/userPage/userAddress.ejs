                                    <h2 style="display: inline; margin: 0;">Your Address</h2>
                                    <a href="/userAddAddress"><button style="background-color: #4CAF50; color: white; padding: 15px; border: none; border-radius: 4px; cursor: pointer;">ADD NEW ADDRESS</button></a>
                                </div>
                                

                                <div class="d-flex flex-wrap " style="justify-content: space-around;">
                                    
                                    
                                        <% if (datas && datas.address && datas.address.length> 0) { %>
                                            <% datas.address.forEach((data,index)=>{ %>

                                                <div class="product__item"
                                                    style="background-color: #dae4dd; width: 100%; padding: 20px; text-align: left;">


                                                      <div style="text-align: center;">

                                                       <h4 for="radioOption1" style="color: rgb(9, 3, 111);">Address <%=index+1%>
                                                        </h4>
                                                      </div>
                                                    <hr style="margin: 0;">

                                                    <li style="margin-bottom: 8px; font-weight: bold;">Name: <%= data.name %>
                                                    </li>
                                                    <li style="margin-bottom: 8px;">Phone: <%= data.phone %>
                                                    </li>
                                                    <li style="margin-bottom: 8px;">Country: <%= data.country %>
                                                    </li>
                                                    <li style="margin-bottom: 8px;">Landmark: <%= data.landmark %>
                                                    </li>
                                                    <li style="margin-bottom: 8px;">City: <%= data.city %>
                                                    </li>
                                                    <li style="margin-bottom: 8px;">Address: <%= data.address %>
                                                    </li>
                                                    <li style="margin-bottom: 8px;">Pincode: <%= data.pincode %>
                                                    </li>

                                                    <hr style="margin: 0;">
                                                    <br>
                                                    <!-- Add more address details as needed -->
                                                    <div style="width: 100%; text-align: center; margin-bottom: 10px;">
                                                        
                                                           <a href="/EditAddAdres?userids=<%= datas._id %>&addressids=<%= data._id %>"> <button
                                                                style="width: 150px; background-color: #113812; color: white; padding: 10px; border: none; border-radius: 4px; cursor: pointer;">EDIT
                                                                ADDRESS</button></a>
                                                        

                                                        

                                                                <form id="deleteForm" action="/deleteAddress" method="post">
                                                                    <input type="hidden" name="userid" value="<%= datas._id %>">
                                                                    <input type="hidden" name="addressid" value="<%= data._id %>">
                                                                    
                                                                    <br>
                                                                    <button type="button" onclick="confirmDelete()" style="width: 150px; background-color: #270b09; color: white; padding: 10px; border: none; border-radius: 4px; cursor: pointer;">
                                                                        DELETE ADDRESS
                                                                    </button>
                                                                </form>
                                                        
                                                    </div>

                                                </div>


                                                <% }); %>
                                                    <% } else { %>
